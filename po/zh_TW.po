#. [message]: speaker=Careonryn
#. [message]: speaker=Dagor
#: Antar/scenarios/04_Through_The_Goblin_Hills.cfg.cfg:303
#: Antar/scenarios/04_Through_The_Goblin_Hills.cfg.cfg:309
msgid "We should stay away from the hills and remain on the path."
msgstr ""

#. [message]: speaker=Careonryn
#. [message]: speaker=Dagor
